<h1>Users</h1>

<div id="users">
  <table class="table table-striped table-hover table-bordered">
    <tr class="table-dark">
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Twitter</th>
      <th>
    </tr>
    <% @users.each do |user| %>
      <%= render user %>
    <% end %>
  </table>
</div>

<%= link_to "New user", new_user_path %>
